public class SplitAt {

public static String[]splitAt(String s, int n){
int split = n;
String first = "";
String second = "";
    for (int i = 0; i<n; i++){
        first += s.charAt(i);
    }

    String [] array = {first,second};
    return array;
}
    
    public static void main(String[] args) {
        
System.out.println(splitAt("hey", 2));
    }

}