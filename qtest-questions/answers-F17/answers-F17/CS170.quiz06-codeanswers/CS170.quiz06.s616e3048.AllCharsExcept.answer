public class AllCharsExcept {
    public static String allCharsExcept(String s, char c){
        String result = "";
        for (int i = 0; i < s.length(); i ++){
            if (s.charAt(i) == c){
                result = result;
            }
            else {
                result += s.charAt(i);
            }
        }
        return result;
    }
    public static void main(String[] args) {
        System.out.println(allCharsExcept("bananas", 'a'));
    }

}