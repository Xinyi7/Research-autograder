public class LengthProduct {

    public static int lengthProduct(String[] x){
        int result = 1;
        for (int i = 0; i < x.length; i++){
            result *= x[i].length();
        }
        return result;
    }
    
    public static void main(String[] args) {
        String[] x1 = {"hello"};
        System.out.println(lengthProduct(x1)); // returns 5
    }

}