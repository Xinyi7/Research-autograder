public class Dot {

    // write your method here
    public static String dot(String s,int k) {
        if (k>=0 && k<s.length()) {
            return s.substring(0,k)+"."+s.substring(k+1);
        } else {
            return s;
        }
    }
    
    public static void main(String[] args) {
        // test your method here
        System.out.println(dot("whatever",5));
    }

}