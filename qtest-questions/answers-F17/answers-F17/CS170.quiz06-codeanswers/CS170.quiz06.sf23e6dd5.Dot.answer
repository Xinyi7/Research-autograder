public class Dot {

    // write your method here
    
    public static void main(String[] args) {
        System.out.println(dot("whatever", 5));// test your method here
    }

    public static String dot(String s, int k) {
        String result = "";
        result += s.substring(0,k) + "." + s.substring(k+1);
        return result;
    }

}