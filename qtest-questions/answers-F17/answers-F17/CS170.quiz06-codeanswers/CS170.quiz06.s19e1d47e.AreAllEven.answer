public static boolean allAllEven(String[] x){

int count = 0;
for (int i = 0; i < x.length; i++){

if (x[i].length()%2==0){
    count= count +1;
}
}

if (x.length==count){
    return true;
    
}else {
    return false;
}
    


    
}





