        // test your method here
        String[] x1 = {"andy", "dave", "carl"};
        String[] x2 = {"eric", "francis", "gary"};
        String[] x3 = {"eric", "francis"};
     System.out.println(allAllEven(x1));
      System.out.println(allAllEven(x2));
