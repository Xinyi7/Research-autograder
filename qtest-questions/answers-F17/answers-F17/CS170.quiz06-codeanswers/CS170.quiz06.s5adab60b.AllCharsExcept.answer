public class AllCharsExcept {

   public static String allCharsExcept(String s, char c) {
    String result = "";
    int i = 0;
    for (i=0; i<s.length(); i++) {
        if (s.charAt(i) != c) {
            result += s.charAt(i);
        }
    }
   return result;
   }
    
    public static void main(String[] args) {
        System.out.println(allCharsExcept("bananas", 'a'));
    }

}