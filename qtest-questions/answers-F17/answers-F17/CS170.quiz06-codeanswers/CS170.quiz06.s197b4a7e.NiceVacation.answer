public class NiceVacation {

    public static String vacation(String s, int n){
        String result = "";
        for (int i = 0; i < n; i++){
            result += s.charAt(i);
        } result += " nice " + s.charAt(n) + " vacation ";
        for (int j = n + 1; j < s.length(); j++){
       result += s.charAt(j);
    }

       return result;
    }
    
    public static void main(String[] args) {
        System.out.println(vacation( "beach", 2));
    }

}