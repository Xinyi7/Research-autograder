public class GrowPoly {

    public static void growPoly(Turtle t, double x, int n, int m) {
      for (int i = 0; i < m; i++) {
        for (int j = 0; j < n; j++) {
            t.forward(x + i * 10);
            t.left(360.0 / n);
        }
        t.forward(x + i * 10);
        t.left(15);
      }
    }
    public static void main(String[] args) {
        Turtle t = new Turtle();
        growPoly(t, 20, 5, 7);
    }
    
}