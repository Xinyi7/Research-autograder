public class GrowPoly {

   public static void f(Turtle t, double x, int n, int m) {

      for (int j = 0; j < m; j++) {
            for (int i = 0; i < n; i++) {
                t.forward(x+10*j);
                t.left(360/n);
            }

        t.forward(x+10*j);
        t.right(15);

        }
   }
    
    public static void main(String[] args) {
        Turtle t = new Turtle();
        f(t, 20, 5, 7);
    }
    
}