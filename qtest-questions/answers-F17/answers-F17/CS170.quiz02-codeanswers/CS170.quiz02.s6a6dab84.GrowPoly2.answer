public class GrowPoly {

public static void growPoly(Turtle t, double x, int n, int m){
for ( int w = 0; w <= m; w++){
polygon(t, x, n, m);
t.forward(x);
x = x + 10;
t.right(15);

}
   
}
public static void polygon(Turtle t, double x, int n, int m){

for (int i = 0; i < 5; i++){
t.forward(x);
t.left(72);
  }
}
    
    public static void main(String[] args) {
       Turtle t = new Turtle();
       growPoly(t, 20, 5, 7);
       
    }
    
}
