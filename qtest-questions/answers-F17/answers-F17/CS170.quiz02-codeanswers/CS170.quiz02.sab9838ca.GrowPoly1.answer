public class growPoly {
    public static void growPoly(Turtle t, double x, int n, int m){
    
    for (int a=1;a<m+1; a++){
    for (int i=0;i<n; i++){
        t.forward(x+(a-1)*10);
        t.left(360.0/n);
        }
        t.forward(x+(a-1)*10);
        t.left(15);
    }
        
    }

    // write your method here
    
    public static void main(String[] args) {
        Turtle t=new Turtle();
        growPoly(t,20,5,7);
        // test your method here
    }
    
}