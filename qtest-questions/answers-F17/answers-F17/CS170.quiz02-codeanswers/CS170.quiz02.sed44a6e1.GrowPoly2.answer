public class GrowPoly(Turtle t, double x, int n, int m) {
    // write your method here
    for (int i = 0; i <= m; i++) {
        for (j = 0; j < n; j++) {
            t.forward(x + (10.0 * m));
            t.left(360.0 / n);
        }
        t.forward(x + (10.0 * m));
        t.right(15);
    }
}

    public static void main(String[] args) {
        // test your method here
        Turtle t = new Turtle();
        GrowPoly(t, 20, 5, 7);
    }
    
}