public class GrowPoly {

    // write your method here
    
    public static void main(String[] args) {
        // test your method here
        Turtle t=new Turtle();
        growpoly(t,20,5,7);
    }
    public static void growpoly(Turtle t, double x, int n, int m){
       
        for(int k=0;k<m;k++){
        for (int i=0;i<n;i++){
            t.forward(x+(k*10));
            t.left(360.0/n);
        }
        t.forward(x+(k*10));
        t.right(15);
    }
    
    }
}