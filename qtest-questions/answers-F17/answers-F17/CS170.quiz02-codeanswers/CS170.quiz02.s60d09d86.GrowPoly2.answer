public class GrowPoly {

    // write your method here
    
    public static void main(String[] args) {
        Turtle t = new Turtle();
        for (int i = 0; i < 10; i++) {
        poly(t, 60, 5);
        t.right(15);
        }
    }



 public static void poly(Turtle t, int n, int m){
        for (int i = 0; i < m; i++) {
            t.forward(n+i);
            t.left(360.0/m);  
        }
    
}



    }
    
   
