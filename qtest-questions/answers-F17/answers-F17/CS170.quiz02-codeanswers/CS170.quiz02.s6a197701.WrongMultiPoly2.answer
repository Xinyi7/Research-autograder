public class MultiPoly {

    public static void multiPoly(Turtle t, double x, int n, int m) {
        for (int k=0;k<m;k++) {
            for (int j = 0; j < n; j++) {
                t.forward(x+(j*10));
                t.left(360.0 / n);
            }
            n=n+1;
        }
    }
    
    public static void main(String[] args) {
        Turtle t = new Turtle();
        t.delay(20);
        multiPoly(t, 100, 5, 8);
    }
}