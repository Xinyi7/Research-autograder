public class GrowPoly {

    // write your method here
    public static void growPoly(Turtle t, double x, int n, int m) {
    for(int w = 0; w < m; w++){
    for(int i = 0; i < n; i++) {
        t.forward(x * w + 10);
        t.left( 360 / n);
    }
    t.forward(x * w + 10);
    t.left(15);
    } 
    }
    public static void main(String[] args) {
    Turtle t = new Turtle();// test your method here
    growPoly(t, 20, 5, 7);
    }
    
}