public class GrowPoly {

    public static void growpoly (Turtle t, double x, int n, int m){
        for(int k = 0; k < m; k++) {
        for(int i = 0; i <= n; i++) {
            t.forward(x + (10 * k));
            t.left(360/ n);
        }
        t.right((360 / n) - 15);
        }
    }
    
    public static void main(String[] args) {
       Turtle t = new Turtle();
       growpoly(t, 20, 5, 7);
    }
    
}