public class GrowPoly {
    public static void growPoly(Turtle t, double x, int n, int m){
       for (int i=0; i< m; i++){
       for (int j=0; j< n; j++){
        t.forward(x + (10 *i));
        t.left(360/ n);
    }
        t.forward(x + (10 *i));
        t.right(15);
       }
    }
    
    public static void main(String[] args) {
        Turtle amy = new Turtle();
        growPoly(amy, 20, 5, 7);
    }
    
}