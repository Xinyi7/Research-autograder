public class GrowPoly {

   public static void growPoly(Turtle t, double x, int n, int m){
for(int a=0; a<m; a++){
    
    for(int i=0; i<n; i++){
    t.forward(x);
    t.left(360.0/n);  
    }
    t.forward(x);
   x=x+10;
   t.left(15);
    
   }
   

   }    
    public static void main(String[] args) {
        Turtle t = new Turtle();
        growPoly(t, 20, 5, 7);
    }
    
}