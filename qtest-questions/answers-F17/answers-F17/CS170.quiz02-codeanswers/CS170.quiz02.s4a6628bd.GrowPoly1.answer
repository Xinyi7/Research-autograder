public class GrowPoly {

    // write your method here
    public static void growPoly(Turtle t, double x, int n, int m){
        for (int k = n; k < m; k++){
            for(int j = 0; j < k; j++){
            t.forward(x+10);
            t.left(360.0/n);
           
        }
    }
    }
}
    
    public static void main(String[] args) {
        // test your method here
        Turtle t = new Turtle (); 
        growPoly(t, 20, 5, 7); 
    }
    
