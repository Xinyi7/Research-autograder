public class GrowPoly {

    // write your method here
    public static void growPoly(Turtle t, double x, int n, int m){
        for (int i=0; i<m; i++){
            for (int j=0; j<n; j++){
                t.forward(x+(10*j));
                t.left(360.0/n);
            }

        }
    }
    
    public static void main(String[] args) {
        // test your method here
        Turtle t = new Turtle();
        growPoly(t, 20, 5, 7);
    }
    
}