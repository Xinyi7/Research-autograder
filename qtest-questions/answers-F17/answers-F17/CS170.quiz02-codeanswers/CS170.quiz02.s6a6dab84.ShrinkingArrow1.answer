public class Arrow {

    public static void arrow(Turtle t, double length, int branches){
for ( int i = 1; i<= branches; i++){
t.right(45);
t.backward(length / i);
t.forward(length / i);
t.left(90);
t.backward(length / i);
t.forward(length / i);
t.right(45);
t.forward(50);  
       }   
     
    }
    
    public static void main(String[] args) {
        Turtle t = new Turtle();
        arrow(t, 150, 5);
    }
    
}