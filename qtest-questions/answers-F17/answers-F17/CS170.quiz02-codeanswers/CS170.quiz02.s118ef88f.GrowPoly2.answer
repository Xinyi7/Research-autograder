public class GrowPoly {

   public static void growPoly(Turtle t, double x, int n, int m){
      for(int k=0; k<7; k++){
        for(int i=0; i<5; i++){
            t.forward(x);
            t.left(60);
        }
   } 
    public static void main(String[] args) {
        Turtle t = new Turtle();
        growPoly(t, 20, 5, 7);
    }
    
}