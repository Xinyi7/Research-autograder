public class GrowPoly {

    public static void polygon(Turtle t, double x, int n, int m) {
        for (int j = 0; j < m; j++) {
            for(int i = 0; i < n; i++) {
            t.forward(x+(j*10));
            t.left(360.0 / n);
         }
            t.forward(x+(j*10));
            t.left(15);
        }
    }
    
    public static void main(String[] args) {
       Turtle dave = new Turtle();
       polygon(dave, 30, 5, 10);
    }
    
}