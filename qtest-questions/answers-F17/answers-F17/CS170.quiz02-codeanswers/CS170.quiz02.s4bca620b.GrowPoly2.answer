public class GrowPoly {

    // write your method here
    public static void growPoly(Turtle t, double x, int n, int m){
        for (int k=0; k<m; k++){
        for (int i=0; i<n; i++){
        t.forward(x+(10*k));
        t.left(360.0/n);
        }
        t.forward(x+(10*k));
        t.right(15);
        }
    }
    
    public static void main(String[] args) {
        // test your method here
        Turtle t= new Turtle();
        t.delay(0);
        growPoly(t, 20, 5, 7);
    }
    
}