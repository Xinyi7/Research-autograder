public class GrowPoly {
    public static void growPoly(Turtle t, double x, int n, int m){
        for (int j = 0; j < m; j ++){
            for (int i = 0; i < n; i ++){
            t.forward(10 * j + x);
            t.left(360.0/ n);
            }
        t.forward(10 * j + x);
        t.left(15);
        }
    }
    public static void main(String[] args) {
        Turtle t = new Turtle();
        growPoly(t, 20, 5, 7);
    }
    
}