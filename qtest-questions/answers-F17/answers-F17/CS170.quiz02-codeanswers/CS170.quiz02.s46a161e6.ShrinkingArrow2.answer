public class Arrow {

public static void arrow(Turtle t, double length, int branches) {
    for(int i=0; i<branches; i++) {
        t.left(45);
        t.forward(length/(i+1));
        t.left(180);
         t.forward(length/(i+1));
        t.right(45);
     
        t.backward(length(i+1));
    }
}

   
    
    public static void main(String[] args) {
      Turtle t = new Turtle();
      arrow(t, 150, 5);
      
    }
    
}