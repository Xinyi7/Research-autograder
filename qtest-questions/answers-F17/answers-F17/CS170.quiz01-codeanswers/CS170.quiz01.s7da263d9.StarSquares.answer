public class StarSquares {

    public static void starSquares(Turtle t, double x) {
        for (int i = 0; i < 5; i++) {
            for (int j = 0; j < 4; j++) {
            t.forward(x/5);
            t.left(90);
        }
       t.forward(x);
       t.right(144);
       }
    }
    
    public static void main(String[] args) {
       
        Turtle dan = new Turtle ();
        starSquares(dan, 100);
        
    }

}