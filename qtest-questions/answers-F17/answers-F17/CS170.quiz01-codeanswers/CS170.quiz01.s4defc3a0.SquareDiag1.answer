public class SquareDiag {

    // write your method here
    
    public static void main(String[] args) {
        Turtle t = new Turtle(); //creates a new turtle
        squareDiag(t, 50, 5); //parameter
    }
    public static void squareDiag (Turtle t, double x, int n){
        for (int j = 0; j < 5; j++){
            for (int i = 0; i < n; i++){
             t.forward(x);
             t.left(90);}
        
     t.forward(x);
     t.right(90);
        
    }
    }

}