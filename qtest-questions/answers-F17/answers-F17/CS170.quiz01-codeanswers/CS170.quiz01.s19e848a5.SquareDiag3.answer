public class squarediag {
     public static void squarediag(Turtle t; double x; int n) {
       for(int k=o; k<n; k++){ 
        for (int j=o; j<4; j++) {
            t.forward(x);
            t.left(90);
            
        }
     }
     }
    // write your method here
    
    public static void main(String[] args) {
        // test your method here
    Turtle t=new Turtle();
    squarediag(t; 50; 4);
    
    }
}