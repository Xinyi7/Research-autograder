public class StarSquares {

    // write your method here
    public static void starSquares(Turtle t, double x){
        for (int i = 0; i < 5; i++) {
            t.forward(x);
            t.right(144);
            for (int j = 0; j < 4; j++){
                t.forward(x / 5);
                t.left(90);
            }
        }

    }
    
    public static void main(String[] args) {
        // test your method here
        Turtle Jim = new Turtle();
        starSquares(Jim, 200);
    }

}