public class ArrowWheel {

    
    
    public static void main(String[] args) {
       Turtle amy = new Turtle();
       arrowWheel(amy, 70.0, 9);
    }
public static void arrowWheel(Turtle t, Double x, int n){
    for (int i=0;i<n;i++){
    t.forward(x);
    t.right(90);
    t.forward(x/10);
    t.left(120);
    t.forward(x/5);
    t.left(120);
    t.forward(x/5);
    t.left(120);
    t.forward(x/10);
    t.left(90);
    t.backward(x);
    t.left(360.0/n);


}
}
}


