public class SquareDiag {

  public static void square(Turtle t, double x, int n){
  for (int i = 0; i < n; i++) {
    for (int i = 0; i < n; i++) {
    t.forward(x);
    t.left(90);
  } 
  
    t.forward(x);
    t.left(90);
    t.forward(x);
    t.right(90);
  
}
  
