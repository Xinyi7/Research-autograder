public class StarSquares {

    // write your method here
    public static void starSquares(Turtle t, double x) {
        for (int k = 0; k < 5; k++) {
        for (int i = 0; i < 4; i++) {
            t.forward(x/5);
            t.left(90);
        }
        t.forward(x);
        t.right(144);
    }
    }
    public static void main(String[] args) {
        // test your method here
        Turtle carl = new Turtle();
        starSquares(carl, 200);
    }

}