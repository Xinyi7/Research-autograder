public class StarSquares {

    public static void square(Turtle t, double x){
        for (int i=0;i<4;i++){
        t.forward((0.2)*x);
        t.left(90);
    }
    }
    public static void starSquares (Turtle t, double x){
        for (int i=0;i<5;i++){
            square(t,x);
            t.forward(x);
            t.right(145);
            
        }
    }
    public static void main(String[] args) {
       Turtle t=new Turtle();
       
       starSquares (t,200);
    }

}