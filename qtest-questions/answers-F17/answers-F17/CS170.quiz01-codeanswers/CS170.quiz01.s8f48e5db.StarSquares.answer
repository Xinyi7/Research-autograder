public class StarSquares {

    // write your method here

    public static void starSquares(Turtle t, double x){
        for (int i=0; i<5; i++){
        t.right(72);
        t.forward(x);
        t.right(72);
            for (int k=0; k<4; k++){
                t.forward(x/5);
                t.right(90);
            }
    }


    }
    
    public static void main(String[] args) {
        // test your method here
        Turtle t = new Turtle();
        starSquares(t, 100);
    }

}