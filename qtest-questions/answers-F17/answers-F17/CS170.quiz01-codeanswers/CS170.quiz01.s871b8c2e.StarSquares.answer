public class StarSquares {

    public static void starSquares(Turtle t, double x)
    {
        for(int i=0;i<5;i++)//draws star
        {
            for(int j = 0;j<4;j++)//draws squares
            {
                t.forward(x/5.0);
                t.left(90);
            }
            t.forward(x);
            t.right(135);
           
        }
    }
    
    public static void main(String[] args)
    {
        Turtle x = new Turtle();
        starSquares(x,100);//calls method
    }

}