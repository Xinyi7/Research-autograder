public class StarSquares {

    public static void starSquares(Turtle t, double x){
        for (int i = 0; i < 5; i++){
            for (int j = 0; j < 4; j++){
                t.forward(x/5.0);
                t.left(90);// write your method here
            }
            t.forward(x);
            t.right(144);
        }
    }
    
    public static void main(String[] args) {
        Turtle don = new Turtle();
        starSquares(don,500);
        // test your method here
    }

}