public class ArrowWheel {

    public static void arrowWheel(Turtle t, double x, int n){
    for (int i=0; i<9; i++){
t.forward(x);
t.right(90);
t.forward(x/10);
t.left(120);
t.forward(x/5);
t.left(120);
t.forward(x/5);
t.left(120);
t.forward(x/10);
t.left(90);
t.backward(x);
t.left(360/9);
    }}
    public static void main(String[] args) {
Turtle t = new Turtle();   
    arrowWheel(t, 70, 9);
    }

}