public class DifferentDivisions {

    public static void divisions(int x, int y){
        return "Look: " + x + " / " + y + " is " + x/y +", but " +  Double.parseDouble(x) + " / " + Double.parseDouble(y) + " is " + Double.parseDouble(x)/Double.parseDouble(y);
    }
    
    public static void main(String[] args) {
        divisions(3, 2);
    }

}