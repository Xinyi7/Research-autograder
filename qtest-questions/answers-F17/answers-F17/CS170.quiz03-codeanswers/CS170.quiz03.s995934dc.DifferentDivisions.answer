public class DifferentDivisions {

    public static String divisions( int x, int y) {
        return "Look: " + (x) + "/" + (y) + " is " + (x-y) + ", but";
        
    }
    
    public static void main(String[] args) {
        System.out.println(divisions(3,2));
    }

}//+ double(x/y) + "is" + (double)(x/y)