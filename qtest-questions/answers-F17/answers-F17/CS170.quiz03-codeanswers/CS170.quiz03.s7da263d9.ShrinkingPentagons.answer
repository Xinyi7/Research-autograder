public class ShrinkingPentagons {

    public static void shrinkingPentagons(Turtle t, double x, int n) {
      for (int j = 1; j <= n; j++) {  
        for (int i = 0; i < 5; i++) {
            t.forward(x/j);
            t.left(72);
        }
        t.penup();
        t.forward(2*(x/j));
        t.pendown();
    }
}
    public static void main(String[] args) {
        Turtle dan = new Turtle ();
        shrinkingPentagons(dan, 100, 5);
    }

}