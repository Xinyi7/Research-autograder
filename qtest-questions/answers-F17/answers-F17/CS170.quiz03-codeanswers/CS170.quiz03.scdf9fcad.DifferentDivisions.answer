public class DifferentDivisions {

    // write your method here
    public static String DifferentDivisions(int x, int y) {
        return "Look: " + x + "/"  + y + "is" + "x / y" + ", but "+ (double) (x)+ "/" + (double) (y) + "is" + (double)(x)/(double)(y);  
    }
    public static void main(String[] args) {
        // test your method here
        System.out.println(DifferentDivisions(3, 2));
    }

}