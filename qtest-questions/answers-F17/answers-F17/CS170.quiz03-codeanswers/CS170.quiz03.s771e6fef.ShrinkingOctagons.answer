public class ShrinkingOctagons {

    // Write your method here
    public static void shrinkingOcatagons(Turtle t, double x, int n){
       for(int j=1; j<=n; j++){
        for(int i=0; i<8; i++){
            t.forward(x/j);
            t.left(45);
        }
        t.penup();
        t.forward(2*x/j);
        t.pendown();
    }
    }
    public static void main(String[] args) {
        // Test your method here
        Turtle t = new Turtle();
        shrinkingOcatagons(t,100,5);
    }

}