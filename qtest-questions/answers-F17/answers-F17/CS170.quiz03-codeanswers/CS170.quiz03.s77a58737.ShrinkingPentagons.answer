public class ShrinkingPentagons {

    public static void shrinkingPentagons (Turtle t, double x, int n){
        double z = x;
    for(int a = 0; a<n; a++){

    for(int i = 0; i<5; i++){
    t.forward(x);
    t.left(360.0/5);
        
    }
    t.penup();
    t.forward(x*2);
    t.pendown();
    x = z / (a + 2);
        
    }
    }
    
    public static void main(String[] args) {
        
  Turtle t=new Turtle();
   shrinkingPentagons(t, 100, 5);

    
   
    }

}