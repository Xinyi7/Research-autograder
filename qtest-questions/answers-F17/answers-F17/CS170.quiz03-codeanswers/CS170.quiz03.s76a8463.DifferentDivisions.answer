public class Divisions {
    // write your method here
    public static String Divisions(int x, int y) {
        return "Look: " + x + " / " + y + " is " + (x/y); //+ " , but " + (x.0) + " / " + (y.0) + " is " + (x.0 / y.0) ; 
    }
    
    public static void main(String[] args) {
        // test your method here
        Divisions(3, 2); 
    }

}