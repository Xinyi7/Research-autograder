public class DifferentDivisions {

    // write your method here
    public static String divisions(int x, int y) {
        return "Look: " + (x) + " / " + (y) + " is " + (x / y) + ", but " + (System.out.println(1.0 * x)) + " / " + (System.out.println(1.0 * y)) + " is " + ((System.out.println(1.0 * x)) / (System.out.println(1.0 * y))); 
    }
    
    public static void main(String[] args) {
        // test your method here
        System.out.println(divisions(3,2));
    }

}