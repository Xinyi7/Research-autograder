public class ShrinkingSquareRose {

    public static void shrinkingSquareRose(Turtle t, double x, double delta, int n){
        for(int i = 0; i < n; i++){
            for(int j = 1; j < 5; j++){
                t.forward(x - ( i*delta ));
                t.left(90);
            }
        t.right(360.0/n);
        }
    }
    
    public static void main(String[] args) {
        Turtle t =  new Turtle();
        shrinkingSquareRose(t,100,10,7);
    }

}