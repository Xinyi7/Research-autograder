public class ShrinkingOctagons {

public static void shrinkingOctagons(Turtle t, double x, int n){
for (int j = 1; j <= n; j++){
for (int i = 0; i < 8; i++){
t.forward(x / j);
t.left(45);
   }
t.penup();
t.forward(x / j);
t.forward(x / j);
t.pendown();   
}
}

   
    public static void main(String[] args) {
 Turtle t = new Turtle();
 shrinkingOctagons(t, 100, 5 );      
    }

}