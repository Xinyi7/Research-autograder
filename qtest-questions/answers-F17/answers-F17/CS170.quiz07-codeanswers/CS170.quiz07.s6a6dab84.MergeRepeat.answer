public class MergeRepeat {

    public static String mergeRepeat(String[] s, int k) {
       int z = 0;
       String result = "";
        for ( int i = 0; i < s.length; i++){
            for (int w = 0; w <= k; w++) {
                result = s[z];
                z++;
            }
        }
        return result;
    }
    
    public static void main(String[] args) {
        String[] x = {"A", "BC"};
        System.out.println(mergeRepeat(x , 3));
    }

}