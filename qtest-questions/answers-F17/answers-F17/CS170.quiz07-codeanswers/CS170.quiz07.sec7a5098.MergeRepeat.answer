public class MergeRepeat {

    // write your method here
    public static String merge(String[] s, int k){
        String result="";
        for (int i=0; i<s.length;i++){
            for (int j=0; j<k; j++){
                result+=s[i];
            }
        }
        return result;
    }
    public static void main(String[] args) {
        // test your method here
        String[] s={"A", "BC"};
        System.out.println(merge(s, 3));
    }

}