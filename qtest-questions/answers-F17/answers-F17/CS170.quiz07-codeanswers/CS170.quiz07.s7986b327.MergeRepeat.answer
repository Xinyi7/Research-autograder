public class MergeRepeat {
  public static String mergerepeat(String s, int k){
    String result = "";
    for (int i = 0; i < k.length; i++){
        for (int j = 0; j < k[i]; j++){
            result += s.charAt(i);
        }
    }
    return result;
  }
