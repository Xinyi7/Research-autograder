public class MergeRepeat {


    public static String [] mergeRepeat (String [] s, int k) {


    String word = "";
        for ( int i = 0; i < s.length; i++) {
            for ( int j = 1; j <+ k; j++) {
            word = word + s[i];
    }
        }
        return word;
    }
    
    public static void main(String[] args) {


     String [] x1 = {"A", "BC"};
     System.out.println(mergeRepeat(x1));
     
    }

}