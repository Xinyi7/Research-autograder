public class MergeRepeat {

    // write your method here
    public static String mergeRepeat(String[] s, int k) {
        String result = 0;
        for (int i = 0; i < k; i++) {
            result += s[i];
        }
        return result;
    }
    
    public static void main(String[] args) {
        // test your method here
        System.out.println(mergeRepeat({"A", "BC"}, 3);
    }

}