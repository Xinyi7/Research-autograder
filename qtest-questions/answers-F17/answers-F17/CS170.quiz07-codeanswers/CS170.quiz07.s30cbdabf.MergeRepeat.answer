public class MergeRepeat {

    public static String mergeRepeat(String[] s, int k){
        String result =  "";
        for(int i = 0; i < s.length; i++){
           result =  result + (s[i] * k);
        }
        return result ;
    }
    public static void main(String[] args) {
     String [] x = {"A", "BC"};
     System.out.println(mergeRepeat(x,3));
    }

}