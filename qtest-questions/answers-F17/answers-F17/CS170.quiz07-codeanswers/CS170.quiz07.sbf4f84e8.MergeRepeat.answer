public class MergeRepeat {

    // write your method here
    public static String mR(String[] s , int k){
        String result = "";
        for (int i=0; i < s.length; i++){
            for (int j = 1; j < k ; j++){
                result += s[i]; 
            }
        }
        return result;
    }
    
    public static void main(String[] args) {
        // test your method here
        mR(["a", "BC"] , 3)
    }

}