public class ChangeLast {

    public static String changeLast(String s, String last){
        String result = s.substring(0, s.length()-1) + last.charAt(last.length()-1);
        return result;
    }
    
    public static void main(String[] args) {
        System.out.println(changeLast("thick", "c"));
        System.out.println(changeLast("root", "palm"));
    }

}