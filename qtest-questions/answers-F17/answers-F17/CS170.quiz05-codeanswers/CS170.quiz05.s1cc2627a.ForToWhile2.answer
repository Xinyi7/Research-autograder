public class ForToWhile {

    public static void main(String[] args) {
        String x = "ABCD";
        int y = 5;
        for (int i = x.length() - 1; i >= 0 ; i -= 2) {
           System.out.println(y + " " + x.charAt(i));
           y--;
           for (int j = y; j > 0; j /= 2) {
               System.out.println("Z " + j);
           }
        }
    }
}