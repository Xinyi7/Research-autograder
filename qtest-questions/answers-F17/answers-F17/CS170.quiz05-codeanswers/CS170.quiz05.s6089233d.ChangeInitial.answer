public class ChangeInitial {

    // write your method here
    public static String changeInitial(String s, String init){
        String result=""+init.charAt(0);
        for (int i=1;i<s.length();i++){
            result += s.charAt(i);
        }
        return result;
    }
    
    public static void main(String[] args) {
        // test your method here
        System.out.println(changeInitial("book","c"));
        System.out.println(changeInitial("root","banana"));
    }

}