public class ForToWhile {

    public static void main(String[] args) {
        String s = "ForWhile";
        int a = 4;
        int b = s.length() - 2;
        while (b <= 6 && b > 2) {
            System.out.println("b=" + b + ": "+ s.charAt(b));
            b -= 2;
            while (b <= 6 && b > 1) {
                System.out.println("i: " + b);
                b -= 3;
            }
        }
    }
}