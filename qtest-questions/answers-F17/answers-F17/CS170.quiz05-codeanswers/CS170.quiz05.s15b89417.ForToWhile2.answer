public class ForToWhile {

    public static void main(String[] args) {
        String x = "ABCD";
        int y = 5;
        int i = x.length() - 1;
        while (i >= 2) {
           System.out.println(y + " " + x.charAt(i));
           i--;
           int j = y;
           while (j > 2) {
               System.out.println("Z " + j);
               j--; 
               
           }
        }
    }
}