public class ForToWhile {

    public static void main(String[] args){
        String x = "ABCD";
        int y = 5;
        int i = x.length()-1;
        int j = y;
        while (i >= 0){
            System.out.println(y + " " + x.charAt(i));
            i -= 2;
              while (j > 0){
                 System.out.println("Z " + j);
                 j /= 2;
        }
      
            
        }
        
    }
}