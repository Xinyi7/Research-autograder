public class ForToWhile {

    public static void main(String[] args) {
        String s = "ForWhile";
        int a = 4;
        while (int b = s.length() - 2) {
        b > 2; 
            System.out.println("b=" + b + ": "+ s.charAt(b));
            a++;
        }
            while (int i = b) {
                System.out.println("i: " + i);
                i++; 
            }
            }
        }
    }
}