public class PigLatin {

    public static String pigLatin(String s) {
        String result = "";
        for(int i = 1; i < s.length(); i++) {
            result += s.charAt(i);
        }
        result += s.charAt(0) + "a";
        return result;
    }
    
    public static void main(String[] args) {
        System.out.println(pigLatin("hello"));
    }

}