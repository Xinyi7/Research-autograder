public class StringBetweenEnd {

    
    public static void main(String[] args) {

    }

    public static String betweenEnd(String s, String between, String end){

        String s = "";

        for(int i=1; i<s.length(); i++;){
            String x = "";
            String between = "";
            String end = "";
            
            x = s.charAt(0) + between + s.charAt(i) + end;
        }

        return x;
        
        
        
    }

}