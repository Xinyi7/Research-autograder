public class ChangeInitial {

 public static String changeInitial(String name, String init){
char c = init.charAt(0);
name.substring(0,1)=c;
return name;

 
    

    
        
    }
    
    public static void main(String[] args) {
     System.out.println(changeInitial("book", "c"));
     System.out.println(changeInitial("root", "banana"));
     
    }

}