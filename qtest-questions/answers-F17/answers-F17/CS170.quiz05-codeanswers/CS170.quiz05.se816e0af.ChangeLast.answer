public class ChangeLast {

    // write your method here
    public static String changeLast(String s, String last){
        String result = "";
        result = s.substring(0, s.length()-1) + last.charAt(last.length()-1);
        return result;
        
        
    }
    
    public static void main(String[] args) {
        // test your method here
        System.out.println(changeLast("book", "c"));
        System.out.println(changeLast("root", "palm"));
        
    }

}