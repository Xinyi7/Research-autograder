public class WhileToFor {

    public static void main(String[] args) {
        int a = 8;
        int b = 4;
        for (int i = a; i > b; i -= 2) {
            int c = i - b;
            System.out.println("hi " + i);
            for (int j = c; j < a; j += 3) {
                System.out.println("bye " + j);
            }
        }
    }
}