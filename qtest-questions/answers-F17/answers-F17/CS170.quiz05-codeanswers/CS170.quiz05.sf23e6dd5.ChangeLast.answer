public class ChangeLast {

    // write your method here
    
    public static void main(String[] args) {
        System.out.println(changeLast("book", "c"));
        System.out.println(changeLast("root", "palm"));// test your method here
    }

    public static String changeLast(String s, String last) {
        String result = s.substring(0, s.length()-1);
        result += last.charAt(last.length()-1);
        return result;
    }
    

}