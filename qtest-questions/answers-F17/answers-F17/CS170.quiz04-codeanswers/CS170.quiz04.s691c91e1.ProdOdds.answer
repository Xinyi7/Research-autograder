public class ProdOdds {

    // write your method here
    public static int prodOdds(int a, int b){
        int product = 1;
        for(int i = a; i<= b; i++){
            if(i%2 == 1){
                product = product*i;
            }else{
                product = product;
            }
        }
        return product;
    }
    public static void main(String[] args) {
        // test your method here
        System.out.println(prodOdds(2,9));
    }

}