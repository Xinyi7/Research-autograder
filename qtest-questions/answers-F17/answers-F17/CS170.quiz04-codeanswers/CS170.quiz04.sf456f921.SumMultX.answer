public class SumMultX {

    public static int sumMultX(int x, int y) {
        int result = 0;
        for (int i = x; i <= y; i += x) {
            result = result + i;
        }
        return result;
    }
    
    public static void main(String[] args) {
        System.out.println(sumMultX(3, 11)); // 18
        System.out.println(sumMultX(3, 12)); // 30
        System.out.println(sumMultX(3, 5)); // 3
        System.out.println(sumMultX(3, 2)); // 0
    }

}