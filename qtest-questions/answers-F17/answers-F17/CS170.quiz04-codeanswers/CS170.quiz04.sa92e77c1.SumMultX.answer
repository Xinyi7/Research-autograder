public class SumMultX {

    public static int sumMultx(int x, int y){
        int result = 0;
        for (int i=x; i<=y; i++){
            if (i%x == 0){
                result += i;
            }
        }
        return result;
    }
    
    public static void main(String[] args) {
        System.out.println(sumMultx(2,9));
        System.out.println(sumMultx(1,3));
        System.out.println(sumMultx(3,7));
        System.out.println(sumMultx(4,12));
    }

}