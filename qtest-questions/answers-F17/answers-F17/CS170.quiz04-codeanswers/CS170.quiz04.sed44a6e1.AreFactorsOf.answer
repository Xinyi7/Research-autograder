public class AreFactorsOf {
    // write your method here
  public static boolean areFactorsOf(int a, int b, int c) {
    if (c % a == 0 && c % b == 0) {
        return true;
    } else {
        return false;
    }
  }


    
    public static void main(String[] args) {
        // test your method here
        System.out.println(areFactorsOf(2, 3, 6)); //return true
        System.out.println(areFactorsOf(3, 1, 27)); //true
        System.out.println(areFactorsOf(3, 2, 5)); //false
    }

}