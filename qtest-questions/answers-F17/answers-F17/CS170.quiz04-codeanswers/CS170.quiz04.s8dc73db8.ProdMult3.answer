public class ProdMult3 {

    // write your method here
    public static int prodMulti3(int a, int b) {
        int result = 1;
        for (int i = a; i <= b; i ++) {
            if (i % 3 == 0) {
                result *= i;
            }
        }
        return result;
    }
    
    public static void main(String[] args) {
        // test your method here
        System.out.println(prodMulti3(5, 10));
        System.out.println(prodMulti3(7, 11));
    }

}