public class SumMultX {

  public static int sumMultX(int x, int y){
    int result=0;
    for (int i=x; i<=y; i++){
        if (i/x>=1&& i%x==0){
            result= result+i;
            
            
         }
        
        }
    return result;
    
    }
    
    public static void main(String[] args) {
        System.out.println(sumMultX(2, 2));
        System.out.println(sumMultX(2, 10));
        System.out.println(sumMultX(5, 6));
        System.out.println(sumMultX(2, 1));
    }

}