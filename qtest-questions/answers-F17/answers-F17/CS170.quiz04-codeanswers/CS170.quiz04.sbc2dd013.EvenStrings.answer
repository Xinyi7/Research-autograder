public class EvenStrings {

    public static boolean evenStrings(String s1, String s2){
        return (s1.length()==s2.length()&&(s1.length()%2==0&&s2.length()%2==0));
    }
    
    public static void main(String[] args) {
        System.out.println(evenStrings("blue", "pink"));
        System.out.println(evenStrings("blue", "red"));
        System.out.println(evenStrings("red","you"));
        System.out.println(evenStrings("banana","pear"));
    }

}