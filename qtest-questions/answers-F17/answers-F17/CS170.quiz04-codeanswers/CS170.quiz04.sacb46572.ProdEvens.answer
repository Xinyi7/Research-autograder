public class ProdEvens {

    // write your method here
    public static int prodEvens(int a, int b) {
        int result = 1;
        for (int i = a; i <= b; i++) {
            result = result * i;
        }
        return result;
    }
    
    public static void main(String[] args) {
        // test your method here
        System.out.println(prodEvens(2, 4));
        System.out.println(prodEvens(1, 6));
        System.out.println(prodEvens(3, 4));
    }

}