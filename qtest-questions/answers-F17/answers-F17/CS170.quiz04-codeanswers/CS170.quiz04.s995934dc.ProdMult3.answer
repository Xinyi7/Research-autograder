public class ProdMult3 {

    public static int prodMult3( int a, int b) {
        int result = 1;
        for (int i = a; i <= b; i++)
         if ( i%3 ==0) {
            result = result * i;
         }
         return result; 
    }
    
    public static void main(String[] args) {
        System.out.println(prodMult3(1, 6)); 
        System.out.println(prodMult3(1, 8)); 
        System.out.println(prodMult3(2, 10)); 
        
    }

}