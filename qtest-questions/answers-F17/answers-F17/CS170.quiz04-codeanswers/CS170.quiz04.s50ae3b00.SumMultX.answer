public class SumMultX {

    public static int sumMultX(int x, int y){
        int result = 0;
        for (int i = x; i <= y; i++){
            if (i % x == 0){
                result += i;
            }  
        }
        return result;
        
    }
    
    public static void main(String[] args) {
        System.out.println(sumMultX(1, 5));
        System.out.println(sumMultX(10, 15));
        System.out.println(sumMultX(10, 100));
        System.out.println(sumMultX(5, 25));
    }

}