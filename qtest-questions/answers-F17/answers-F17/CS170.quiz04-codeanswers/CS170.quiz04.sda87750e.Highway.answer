public class Highway {

    public static String highway(double speed) {
        if(speed < 40) {
            return "annoying";
        } else if(speed > 70) {
            return "dangerous";
        } else {
            return "OK";
        }
    }
    
    public static void main(String[] args) {
        System.out.println(highway(15));
        System.out.println(highway(40));
        System.out.println(highway(69));
        System.out.println(highway(71));
    }

}