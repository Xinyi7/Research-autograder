public class ProdEvens {

    public static int prodEvens(int a, int b) {
        int result = 1;
        for (int i = a; i <= b; i++) {
            if (i % 2 == 0) {
                result *= i;
            }
        }
        return result;
    }
    
    public static void main(String[] args) {
        System.out.println(prodEvens(1, 5)); //8
        System.out.println(prodEvens(2, 7)); //48
    }

}