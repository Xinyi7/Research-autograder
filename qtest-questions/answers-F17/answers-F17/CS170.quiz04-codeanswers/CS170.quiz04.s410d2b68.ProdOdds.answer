public class ProdOdds {

    // write your method here
    public static int prodOdds(int a, int b){
        int result=1;
        for (int i=a; i<=b; i++){
            result=result*i;   
        }
        return result;
    }
    
    public static void main(String[] args) {
        // test your method here
        System.out.println(prodOdds(1,3));
         System.out.println(prodOdds(1,5));
    }

}