public class ProdMult3 {

    public static int prodMult3(int a, int b) {
        int result = 1;
     for (int i= a; i<=b; i+=3) {   
      result *= i; 
    }
    return result; 
    }
    
    public static void main(String[] args) {
      System.out.println(prodMult3(3,6));
      System.out.println(prodMult3(2,7));
      System.out.println(prodMult3(1,9));
        }

    }
