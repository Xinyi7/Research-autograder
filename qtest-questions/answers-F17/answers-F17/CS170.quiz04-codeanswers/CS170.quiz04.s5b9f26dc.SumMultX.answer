public class SumMultX {

    public static int sumMultX(int x, int y){
        int result = 0;
        for(int i = 0; i <= y; i++){
           if(i % x == 0){ 
            result += i;
           }
        }
        return result;
    }
    
    public static void main(String[] args) {
        System.out.println(sumMultX(3, 9));
        System.out.println(sumMultX(5, 20));
    }

}