public class AreFactorsOf {

    public static boolean areFactorsOf(int a, int b, int c)
    {
        boolean check = false;

        if(c%a==0 && c%b==0)
        {
            check = true;
        }

        return check;
    }
    
    public static void main(String[] args) {

        System.out.println(areFactorsOf(3,2,6));
        System.out.println(areFactorsOf(3,1,27));
        System.out.println(areFactorsOf(3,2,5));
    }

}