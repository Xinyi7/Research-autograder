public class ProdOdds {

    // write your method here
    public static int prodOdds(int a, int b){
        int prod = 1;
        for (int i = a; i<=b; i++){
            if (i%2==1){
                prod *=  i;
            }
        }
        return prod;
    }
    public static void main(String[] args) {
        // test your method here
        System.out.println(prodOdds(2,4));
        System.out.println(prodOdds(2,3));
        System.out.println(prodOdds(1,5));
    }

}