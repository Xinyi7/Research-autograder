public class AreMultiplesOf {

    public static boolean areMultiplesof(int a, int b, int c){
        if (a%c == 0 && b%c == 0){
            return true;
        }else{
            return false;
        }
    }
    
    public static void main(String[] args) {
        System.out.print(areMultiplesof(10, 6, 2));
        System.out.print(areMultiplesof(10, 6, 3));
        System.out.print(areMultiplesof(10, 6, 5));
    }

}