public class ProdOdds {

    // write your method here
    
    public static void main(String[] args) {
        System.out.println(prodOdds(3,7));
         System.out.println(prodOdds(1,5));// test your method here
    }

    public static int prodOdds(int a, int b) {
        int result = 1;
        for (int i = a; i <= b; i++) {
            if (i % 2 != 0) {
                result *= i;
            }
        }
        return result;
    }

}