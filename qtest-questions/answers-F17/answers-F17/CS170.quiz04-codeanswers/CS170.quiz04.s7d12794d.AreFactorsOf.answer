public class AreFactorsOf {
    public static boolean areFactorOf(int a, int b, int c){
        
        if (c % a=0 && c % b=0){
            return true;
        }
        else{
            return false;
        }
    }
    
    public static void main(String[] args) {
        areFactorsOf(3,2,6);
        areFactorsOf(3,1,27);
        areFactorsOf(3,2,5);
    }

}