public class ProdMult3 {

public static int prodMult3 (int a, int b){
    int result = 0;
    for (int i = a; i<=b; i++){
    if (i % 3 == 0){
    
        result +=i;
    }
    }
    return result;
}
    
    public static void main(String[] args) {
System.out.println(prodMult3(3,9));
System.out.println(prodMult3(2,7));
System.out.println(prodMult3(4,5));


    }

}