public class Highway {

    public static String highway (double speed){
        if (speed < 40.0){
            return "annoying";
        }
        else if (speed > 70.0){
            return "dangerous";
        }
        else{
            return "ok";
        }
    }
    
    public static void main(String[] args) {
        System.out.println(highway(35.2)); //annoying
        System.out.println(highway(55.0)); //ok
        System.out.println(highway(87.0)); //dangerous
    }

}