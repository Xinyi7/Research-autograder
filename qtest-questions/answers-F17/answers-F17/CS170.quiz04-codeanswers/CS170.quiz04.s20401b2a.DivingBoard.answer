public class DivingBoard {

    public static void divingBoard(double height) {
        if (height <= 1.5) {
            System.out.println("boring");
        } else if (height >= 5) {
            System.out.println("no way");
        } else {
            System.out.println("weee");
        }
    }
    
    public static void main(String[] args) {
        divingBoard(1);
        divingBoard(1.5);
        divingBoard(3);
        divingBoard(5);
        divingBoard(7);
    }

}