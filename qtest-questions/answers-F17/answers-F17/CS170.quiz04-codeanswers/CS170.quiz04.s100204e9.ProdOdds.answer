public class ProdOdds {

    // write your method here
    
    public static void main(String[] args) {
        // test your method here
        System.out.println(prodOdds(2,7));
    }
    public static int prodOdds(int a, int b) {
        int result = 1;
        for (int i = a; i<=b;i += 2) {
            result = result * i;
        }
        return result;
    }
}