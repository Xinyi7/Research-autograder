public class BodyType {
   
    public static String bodyType(double bmi) {
        if (bmi < 18.5) {
        return "underweight";
        } else if (bmi > 25) {
        return "overweight";
        } else {
        return "normal";
        }
    }

    // write your method here
    
    public static void main(String[] args) {
    System.out.println(bodyType(2));
    System.out.println(bodyType(18.5));
    }

}