public class ProdOdds {

    public static int prodOdds(int a, int b){
 int result = 1;
for ( int i = a; i <= b; i++){
   if ( i % 2 != 0 ) {
result = result * i;     
   }
 
}
return result;
    }     

    
    public static void main(String[] args) {
        System.out.println(prodOdds(1,7));
    }

}
