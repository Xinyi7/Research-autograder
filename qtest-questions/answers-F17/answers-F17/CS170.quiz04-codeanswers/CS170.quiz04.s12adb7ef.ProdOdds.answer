public class ProdOdds {

    // write your method here
    public static int prodOdds(int a, int b){
        int x=0;
        for(int i=a;i<=b;i++){
            if(i%2!=0){
            x=x+i;
            }
            }
        return x;
    }
    public static void main(String[] args) {
        // test your method here
        System.out.println(prodOdds(5,10));
        System.out.println(prodOdds(4,7));
        System.out.println(prodOdds(1,11));
    }

}