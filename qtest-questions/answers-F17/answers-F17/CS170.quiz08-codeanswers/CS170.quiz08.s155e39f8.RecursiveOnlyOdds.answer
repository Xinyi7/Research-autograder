public class RecursiveOnlyOdds {

   public static boolean onlyOdds(int[] a, int n) {

    if ( n == 0 ) {
        return false;
    }
    else ( a[n-1] % 2 == 1) {
        return onlyOdds( a, n-1);
    }
    }
    
    public static void main(String[] args) {

       int [] w = {1, 3, 5};
       onlyOdds(w, 3);
    }

}