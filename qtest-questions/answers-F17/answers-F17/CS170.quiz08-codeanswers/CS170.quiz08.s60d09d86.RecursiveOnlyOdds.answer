public class RecursiveOnlyOdds {
public static String onlyOdds  (int []a, int n){
    
  if ( a.length % n == 0) { System.out.println("true");
  return "true"; }
     else return onlyOdds(a, 1); }
 
    public static void main(String[] args) {
        // test your method here
        int [] w = {1,3,5};
        int [] x = {1};
        onlyOdds(x,1);
        onlyOdds(w,3);
    }

}